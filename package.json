{
  "name": "hackaton-app",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "expo start --tunnel",
    "mock": "json-server --watch db.json --host 0.0.0.0 --port 3333",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "coverage:open": "node -e \"const cp=require('child_process');const p='coverage/lcov-report/index.html'; if(process.platform==='win32'){cp.exec(`start \\\"\\\" ${p.replaceAll('/', '\\\\')}`)} else if(process.platform==='darwin'){cp.exec(`open ${p}`)} else {cp.exec(`xdg-open ${p}`)}\"",
    "coverage:missing": "node scripts/coverage-missing.mjs"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-navigation/native": "^7.1.6",
    "expo": "~53.0.20",
    "expo-font": "~13.3.2",
    "expo-linking": "~7.1.7",
    "expo-router": "~5.1.4",
    "expo-splash-screen": "~0.30.10",
    "expo-status-bar": "~2.2.3",
    "expo-system-ui": "~5.0.10",
    "expo-web-browser": "~14.2.0",
    "react": "19.0.0",
    "react-dom": "19.0.0",
    "react-native": "0.79.5",
    "react-native-reanimated": "~3.17.4",
    "react-native-safe-area-context": "5.4.0",
    "react-native-screens": "~4.11.1",
    "react-native-web": "~0.20.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@testing-library/jest-native": "^5.4.3",
    "@testing-library/react-native": "^13.3.3",
    "@types/jest": "29.x",
    "@types/react": "~19.0.10",
    "jest": "^29.7.0",
    "jest-expo": "~53.0.9",
    "json-server": "^1.0.0-beta.3",
    "react-test-renderer": "19.0.0",
    "typescript": "~5.8.3"
  }
}
